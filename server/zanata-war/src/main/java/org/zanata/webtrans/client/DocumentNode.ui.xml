<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:f="urn:import:org.zanata.webtrans.client">

	<ui:style>
		.unitStatus {
			float: left;
			padding-right:25px;
			width: 120px;
			height: 1.2em;
			top:3px;
		}
		
	</ui:style>
	<g:HorizontalPanel>
        <g:FlowPanel>
			<f:TransUnitCountGraph ui:field='transUnitCountGraph' styleName="{style.unitStatus}" />
	   </g:FlowPanel>
	</g:HorizontalPanel>
	
</ui:UiBinder> 