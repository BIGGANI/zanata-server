<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
        .container
        {
			height:100%;
			margin:0 auto;
		}
		
	</ui:style>
	
	<g:HorizontalPanel styleName="{style.container}" verticalAlignment="ALIGN_MIDDLE" ui:field="container">
		<g:cell width="16px">
			<g:Image ui:field="image"/>  
		</g:cell>
		<g:cell>
			<g:InlineLabel ui:field="label"/>
		</g:cell>
		<g:cell width="16px">
			<g:Image ui:field="image2"/>  
		</g:cell>
	</g:HorizontalPanel>
</ui:UiBinder>