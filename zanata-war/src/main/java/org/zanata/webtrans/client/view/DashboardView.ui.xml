<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field='resources' type="org.zanata.webtrans.client.resources.Resources"/>

    <ui:style>
    	.container
    	{
			background:#E0E8EE;
			margin:15px;
		}
        .rootTable
        {
			background:#E0E8EE;
		}
    </ui:style>
    
    
    <g:LayoutPanel styleName="{style.container}">
    	<g:layer top="0" bottom="0">
			<g:FlexTable ui:field="rootTable" styleName="{style.rootTable}" cellPadding="10"/>
    	</g:layer>
    </g:LayoutPanel>
    
    
</ui:UiBinder> 